<?xml version="1.0" encoding="UTF-8"?>
<config>
	<systemwide/>
	<machine/>
	<dcf>
		<keyword info="Mediator credentials: credentials {username},{password}" name="credentials">
			<param type="String">wsuser</param>
			<param type="String">wspass</param>
		</keyword>
		<keyword name="type">
			<param type="String">componentUploadUTSRunner</param>
		</keyword>
		<keyword name="threads">
			<param type="Integer">10</param>
		</keyword>	
		<keyword name="script">
			<param type="String">/opt/evertz/mediator/etc/runners/run_componentUploadUTS.js</param>
		</keyword>	
		<keyword info="Sets the umask of the environment that the script is run in, optional" name="umask">
			<param type="String">0002</param>
		</keyword>
	</dcf>
	<alarm/>
</config>
